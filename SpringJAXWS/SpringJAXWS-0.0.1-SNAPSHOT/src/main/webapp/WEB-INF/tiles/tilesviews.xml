<!DOCTYPE tiles-definitions PUBLIC
"-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
"http://tiles.apache.org/dtds/tiles-config_2_1.dtd">
<tiles-definitions>
	<definition name="template" template="/jsp/views/layout/main_template.jsp">
		<put-attribute name="title" value="Home Page"></put-attribute>
		<put-attribute name="header" value="/jsp/views/layout/top.jsp" />
		<put-attribute name="side-panel" value="/jsp/views/layout/side.jsp" />
		<put-attribute name="footer" value="/jsp/views/layout/footer.jsp" />
		<put-attribute name="content" value="" />
	</definition>
	<definition name="home" extends="template">
		<put-attribute name="content" value="/jsp/views/home.jsp" />
	</definition>
	<definition name="simpleWebService" extends="template">
		<put-attribute name="content" value="/jsp/views/results/simpleWebService.jsp" />
	</definition>
</tiles-definitions>